{
  "name": "@recipe/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "generate": "npx tsx scripts/generate-types.ts",
    "ios": "next build --turbopack && npx cap sync && npx cap open ios",
    "increment-version": "npx tsx scripts/increment-version.ts",
    "mobile": "next build --turbopack && npx cap sync && npm run apk && npm run open-apk",
    "store": "npm run increment-version && next build --turbopack && npx cap sync && npm run apk && npm run open-aab",
    "apk": "cd android && gradlew.bat assembleDebug && gradlew.bat bundleRelease && cd ..",
    "open-apk": "start android\\app\\build\\outputs\\apk\\debug",
    "aab": "npm run increment-version && cd android && gradlew.bat bundleRelease && cd ..",
    "open-aab": "start android\\app\\build\\outputs\\bundle\\release",
    "clean": "cd android && gradlew.bat clean"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/share": "^7.0.2",
    "@capacitor/status-bar": "^7.0.3",
    "@clerk/clerk-react": "^5.58.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.86.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.556.0",
    "next": "16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "send-intent": "^7.0.0",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "supabase": "^2.65.6",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
